/* This is where the tables are created */


  CREATE TABLE Interviews (
  Interview_ID Number(10),
  Student_ID Number(10),
  Staff_ID Number(10),
  Interview_Date Date,
  Interview_Time Varchar(20),
  Int_Comment Varchar(100),
  constraint pk_Interviews PRIMARY KEY (Interview_ID)
  );

CREATE TABLE Staff (
  Staff_ID Number(10),
  Staff_First_Name Varchar(15),
  Staff_Last_Name Varchar(15),
  Staff_DOB Date,
  Staff_Tele_Num Char(11),
  Staff_Address Varchar(100),
  Department Varchar(15),
  Line_Manager Varchar(50),
  Base_Pay Number(10),
  constraint pk_Staff PRIMARY KEY (Staff_ID)
  );

CREATE TABLE Skills (
  Skill_ID Varchar(10),
  Staff_ID Number(10),
  Skill_Name Varchar(20),
  Pay_Increase Number(10),
  constraint pk_Skills PRIMARY KEY (Skill_ID)
  );

CREATE TABLE Staff_Absence (
  Absence_ID Number(10),
  Staff_ID Number(10),
  Start_Date Date,
  Return_Date Date,
  Reason Varchar(30),
  constraint pk_Staff_Absence PRIMARY KEY (Absence_ID)
  );

CREATE TABLE International_Students (
  Student_ID Number(10),
  Passport_No Number(9),
  Visa_No Number(9),
  Origin_Country Varchar(56)
  );

CREATE TABLE Courses (
  Course_ID Number(10),
  Staff_ID Number(10),
  Course_Name varchar(30),
  Unit_Name varchar(30),
  Room_ID Number(10),
  Unit_Leader_ID Number(10),
  constraint pk_Courses PRIMARY KEY (Course_ID)
  );

CREATE TABLE Fees (
  Student_ID Number(10),
  Initial_Fee Number(10),
  Balance Number(5),
  Amount_Paid Number(5)
  );

CREATE TABLE Distanced_Learning_Students (
  Student_ID Number(10),
  Academic_Tutor Varchar(50),
  Pastoral_Tutor Varchar(50)
  );
  
CREATE TABLE Students (
  Student_ID Number(10),
  Student_First_Name Varchar(15),
  Student_Last_Name Varchar(15),
  Student_DOB Date,
  Student_Type Varchar(2),
  Student_Tele_Num Char(11),
  Student_Address Varchar(100),
  constraint pk_Students PRIMARY KEY (Student_ID)
  );

CREATE TABLE Payroll (
  id NUMBER GENERATED BY DEFAULT AS IDENTITY (START WITH 1000 INCREMENT BY 1),
  Staff_ID Number(10),
  Payroll_Total Number(10,2),
  Tax number(10,2),
  Pay_Date Date
  );
  

CREATE TABLE Attendance (
  Attendance_ID Number(10),
  Student_ID Number(10),
  Course_ID Number(10),
  Attendance_Date Date,
  Attendance_Mark Char(1),
  constraint pk_Attendance PRIMARY KEY (Attendance_ID)
  );

CREATE TABLE Locker (
  Locker_ID Number(5),
  Student_ID Number(10),
  Deposit_Paid Char(1)
  );
  
  
/* This is where the foreign keys are designated to the related tables */

ALTER TABLE International_Students
ADD CONSTRAINT fk_Student_ID1
FOREIGN KEY (Student_ID)
REFERENCES Students(Student_ID); 

ALTER TABLE Locker
ADD CONSTRAINT fk_Student_ID2
FOREIGN KEY (Student_ID)
REFERENCES Students(Student_ID);

ALTER TABLE Attendance
ADD CONSTRAINT fk_Student_ID3
FOREIGN KEY (Student_ID)
REFERENCES Students(Student_ID);

ALTER TABLE Fees
ADD CONSTRAINT fk_Student_ID4
FOREIGN KEY (Student_ID)
REFERENCES Students(Student_ID);

ALTER TABLE Interviews
ADD CONSTRAINT fk_Student_ID5
FOREIGN KEY (Student_ID)
REFERENCES Students(Student_ID);

ALTER TABLE Distanced_Learning_Students
ADD CONSTRAINT fk_Student_ID6
FOREIGN KEY (Student_ID)
REFERENCES Students(Student_ID);

ALTER TABLE Attendance
ADD CONSTRAINT fk_Course_ID1
FOREIGN KEY (Course_ID)
REFERENCES Courses(Course_ID);

ALTER TABLE Courses
ADD CONSTRAINT fk_Staff_ID1
FOREIGN KEY (Staff_ID)
REFERENCES Staff(Staff_ID);

ALTER TABLE Interviews
ADD CONSTRAINT fk_Staff_ID2
FOREIGN KEY (Staff_ID)
REFERENCES Staff(Staff_ID);

ALTER TABLE Skills
ADD CONSTRAINT fk_Staff_ID3
FOREIGN KEY (Staff_ID)
REFERENCES Staff(Staff_ID);

ALTER TABLE Payroll
ADD CONSTRAINT fk_Staff_ID4
FOREIGN KEY (Staff_ID)
REFERENCES Staff(Staff_ID);

ALTER TABLE Staff_Absence
ADD CONSTRAINT fk_Staff_ID5
FOREIGN KEY (Staff_ID)
REFERENCES Staff(Staff_ID);

